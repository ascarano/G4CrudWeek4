<%= form_for @event do |f| %>
    <%= f.label :description %>
    <%= f.text_field :description %><br>
    <%= f.label :loacation %>
    <%= f.text_field :location %><br>
    <%= f.label :capacity %>
    <%= f.number_field :capacity %><br>
    <%= f.label :date %>
    <%= f.date_field :date %><br>
    <%= f.label :requires_id %>
    <%= f.check_box :requires_id %><br>
    <%= f.submit %>
  <% end %>
