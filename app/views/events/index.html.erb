<%= link_to 'Create Event', new_event_path %>
<%= notice %>
<table>
  <thead>
    <tr>
      <th>
        Event
      </th>
      <th>
        Capacity
      </th>
      <th>
        Date
      </th>
      <th>

      </th>
    </tr>
  </thead>
  <tbody>
    <% @events.each do |event| %>
    <tr>
      <td>
        <%= event.description %>
      </td>
      <td>
        <%= event.capacity %>
      </td>
      <td>
        <%= event.date %>
      </td>
      <td>
        <%= link_to "edit", edit_event_path(event) %>
      </td>
      <% end %>
    </tr>
  </tbody>
</table>
